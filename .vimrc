map {<Space> {<Space><Space>}<left><left>
"imap { {<CR>}
"imap {<CR> {<CR><CR>}<Up><Esc>==i
imap {<CR> {<CR>}<Up><End><CR>

inoremap " ""<left>
inoremap "" ""

filetype indent on
set nowrap
set tabstop=4
set shiftwidth=4
set expandtab
set smartindent
set autoindent
set cindent

set showmatch
set number
set clipboard=unnamedplus
set wrap
